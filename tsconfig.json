{
	"compilerOptions": {
		"target": "ES6",
		"strict": false,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "nodenext",
		"outDir": "./dist",
		"module": "NodeNext",
		"allowJs": true
	},
	"include": ["src"],
	"exclude": ["node_modules"]
}
